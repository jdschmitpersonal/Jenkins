pipeline{
    agent any
        stages {
            stage("one") {
                steps {
                    sh 'echo "Script is running from $HOSTNAME"'
                    sh 'cat /var/log/secure | grep -E "root" | grep -Ei "session" > /var/log/rootsessions'
                    sh 'cat /var/log/rootsessions'
                    
                }
            }
        }
}
